{
  "name": "c4ys/doctrine-snowflakes-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides an Snowflake-ID Generator for Doctrine",
  "keywords": [
    "snowflake",
    "doctrine",
    "symfony",
    "symfony5",
    "symfony-bundle",
    "bundle"
  ],
  "homepage": "https://gitlab.com/kaigrassnickpublic/symfony/bundles/doctrine-snowflake-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Kai Grassnick",
      "email": "info@kai-grassnick.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "KaiGrassnick\\DoctrineSnowflakeBundle\\": ""
    },
    "exclude-from-classmap": []
  },
  "require": {
    "php": "^7.4|^8.0",
    "kaigrassnick/snowflake-bundle": "^1.0",
    "symfony/framework-bundle": "^5.0",
    "symfony/orm-pack": "^2.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
